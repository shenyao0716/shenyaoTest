<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>loginServ</servlet-name>
    <servlet-class>com.MeetingRoomMS.Servlet.loginServ</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getRoomServ</servlet-name>
    <servlet-class>com.MeetingRoomMS.Servlet.getRoomServ</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getAllUsersInfoServ</servlet-name>
    <servlet-class>com.MeetingRoomMS.Servlet.getAllUsersInfoServ</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>updateUserInfoServ</servlet-name>
    <servlet-class>com.MeetingRoomMS.Servlet.updateUserInfoServ</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>deleteUserInfoServ</servlet-name>
    <servlet-class>com.MeetingRoomMS.Servlet.deleteUserInfoServ</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>loginServ</servlet-name>
    <url-pattern>/loginServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getRoomServ</servlet-name>
    <url-pattern>/getRoomServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getAllUsersInfoServ</servlet-name>
    <url-pattern>/getAllUsersInfoServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateUserInfoServ</servlet-name>
    <url-pattern>/updateUserInfoServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteUserInfoServ</servlet-name>
    <url-pattern>/deleteUserInfoServ</url-pattern>
  </servlet-mapping>




   
   <welcome-file-list>
        <welcome-file>JSP/index.jsp</welcome-file>      
   </welcome-file-list>
  

</web-app>